    );

    INSERT INTO [#LuckyBetgameWager]
    (
        [BetslipId],
        [Date],
        [Amount],
        [TransactionType],
        [TransactionTypeCategory],
        [NoOfBets]
    )
    SELECT [BetslipId],
           MIN([Date]) AS [Date],
           SUM(-[Amount]) AS [Amount],
           [TransactionType],
           [TransactionTypeCategory],
           1 AS [NoOfBets]
    FROM [#PayoutWagers]
    WHERE [TransactionTypeCategory] = ''Wager''
          AND [TransactionType] IN ( ''BetGames Bet Wager'', ''BetGames Wager Reversal'', ''Lucky Numbers Wager'',
                                     ''Lucky Numbers Wager Reversal'', ''Live Games Wager'', ''Live Games Wager Refund'',
									 ''Casino Games Wager'', ''Casino Games Wager Refund'',
                                      ''Virtual Wager'', ''Virtual Wager Reversal'' 
                                   )
    GROUP BY [BetslipId],
             [TransactionType],
             [TransactionTypeCategory];



    INSERT INTO [#ALLWagers]
    (
        [BetslipId],
        [Date],
        [Amount],
        [TransactionType],
        [TransactionTypeCategory],
        [SettlementStatus],
        [NoOfBets],
        [ResultingStatus]
    )
    SELECT [sw].[BetslipId],
           [Date],
           [Amount],
           [TransactionType],
           [TransactionTypeCategory],
           [sw].[SettlementStatus],
           [sw].[NoOfBets],
           NULL AS [ResultingStatus]
    FROM [#SportsWager] AS [sw]
    UNION ALL
    SELECT [lbg].[BetslipId],
           [Date],
           [Amount],
           [TransactionType],
           [TransactionTypeCategory],
           [TransactionTypeCategory],
           1 AS [NoOfBets],
           ISNULL([betstat].[BetStatusDescription], [stat].[Status]) AS [ResultingStatus]
    FROM [#LuckyBetgameWager] AS [lbg]
        LEFT JOIN [VuvuzelaZA].[BetGames].[BetGamesBet] AS [bg]
            ON [lbg].[BetslipId] = [bg].[BetGamesBetId]
        LEFT JOIN [VuvuzelaZA].[Bitville].[Betslip] AS [bs]
            ON [bs].[BetslipId] = [lbg].[BetslipId]
        LEFT JOIN [VuvuzelaZA].[Bitville].[Status] AS [stat]
            ON [stat].[StatusCode] = [bs].[StatusCode]
        LEFT JOIN [VuvuzelaZA].[BetGames].[BetStatus] AS [betstat]
            ON [betstat].[BetStatusCode] = [bg].[BetStatusCode];





