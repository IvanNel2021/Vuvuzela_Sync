    INSERT INTO [#WinningStake]
    (
        [WinningStake],
        [Date],
        [TransactionTypeCategory],
        [BetslipId]
    )
    SELECT SUM(-[Payout].[Amount]) AS [WinningStake],
           [Payout].[Date],
           [Payout].[TransactionTypeCategory],
           [Payout].[BetslipId]
    FROM
    (
        SELECT SUM(ISNULL([TL].[Amount], 0)) AS [Amount],
               [P].[Date],
               [tt].[Description] AS [TransactionTypeCategory],
               [P].[InternalReferenceId] AS [BetslipId]
        FROM [#Payout] AS [P]
            JOIN [VuvuzelaZA].[dbo].[TransactionLog] AS [TL] (NOLOCK)
                ON [TL].[InternalReferenceId] = [P].[InternalReferenceId]
            JOIN [VuvuzelaZA].[dbo].[TransactionType] AS [tt]
                ON [tt].[TransactionTypeId] = [TL].[TransactionTypeId]
            JOIN [VuvuzelaZA].[dbo].[TransactionTypeCategory] AS [ttc]
                ON [ttc].[TransactionTypeCategoryId] = [tt].[TransactionTypeCategoryId]
        WHERE tt.TransactionTypeCategoryId = ''00000000-0000-0000-DA7A-000000500001''
              AND tt.BalanceIndexAffected = 0
		and NOT EXISTS (SELECT TOP 1 1 FROM VuvuzelaZA.synapse.betslip	  SB WHERE 	tl.InternalReferenceId = Sb.InternalReference) --Remove horse racing
        GROUP BY [P].[Date],
                 [tt].[Description],
                 [P].[InternalReferenceId]

    ) AS [Payout]
    GROUP BY [Payout].[Date],
             [Payout].[TransactionTypeCategory],
             [Payout].[BetslipId];





