                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">EXEC ('

  DECLARE @StartDate DATE,
            @EndDate DATE,
            @LastArcEntry DATETIME,
            @VATPerc DECIMAL(5, 3),
            @vLogText VARCHAR(MAX);

    SET @StartDate = GETDATE() - 7
    SET @EndDate = GETDATE();
	
  
	CREATE TABLE #Merge
    (
        [Date] [DATE] NULL,
        [Number] [INT] NULL,
        [Stake] [DECIMAL](38, 4) NULL,
        [WinningStake] [DECIMAL](38, 4) NULL,
        [Payout] [DECIMAL](38, 4) NULL,
        [GGR] [DECIMAL](38, 4) NULL,
        [Tax Amount] [NUMERIC](38, 6) NULL,
        [VAT] [NUMERIC](38, 6) NULL,
        [GameType] [VARCHAR](12) NOT NULL,
        [ResultingStatus] [VARCHAR](50) NULL
    );


	CREATE TABLE [#Wagers]
    (
        [BetslipId] UNIQUEIDENTIFIER NOT NULL,
        [Date] [DATETIME] NULL,
        [Amount] [DECIMAL](38, 4) NULL,
        [TransactionTypeCategory] [VARCHAR](50) NULL,
        [SettlementStatus] [VARCHAR](50) NULL,
        [NoOfBets] INT NULL,
        [TransactionType] [VARCHAR](50) NULL
    );


    CREATE TABLE [#PayoutWagers]
    (
        [BetslipId] [VARCHAR](50) NOT NULL,
        [Date] [DATETIME] NULL,
        [Amount] [DECIMAL](38, 4) NULL,
        [TransactionType] [VARCHAR](50) NULL,
        [TransactionTypeCategory] [VARCHAR](50) NULL
    );

    CREATE TABLE [#SportsWager]
    (
        [BetslipId] [VARCHAR](50) NOT NULL,
        [Date] [DATETIME] NULL,
        [Amount] [DECIMAL](38, 4) NULL,
        [TransactionType] [VARCHAR](50) NULL,
        [TransactionTypeCategory] [VARCHAR](50) NULL,
        [SettlementStatus] [VARCHAR](50) NULL,
        [NoOfBets] INT NULL
    );
    CREATE TABLE [#ALLWagers]
    (
        [BetslipId] [VARCHAR](50) NOT NULL,
        [Date] [DATETIME] NULL,
        [Amount] [DECIMAL](38, 4) NULL,
        [TransactionType] [VARCHAR](50) NULL,
        [TransactionTypeCategory] [VARCHAR](50) NULL,
        [SettlementStatus] [VARCHAR](50) NULL,
        [NoOfBets] INT NULL,
        [ResultingStatus] [VARCHAR](50) NULL
    );

    CREATE TABLE [#Payout]
    (
        [InternalReferenceId] [VARCHAR](50) NOT NULL,
        [Amount] [DECIMAL](38, 4) NULL,
        [Date] [DATETIME] NULL
    );
    CREATE TABLE [#WinningStake]
    (
        [WinningStake] [DECIMAL](38, 4) NULL,
        [Date] [DATETIME] NULL,
        [TransactionTypeCategory] [VARCHAR](50) NULL,
        [BetslipId] [VARCHAR](50) NOT NULL,
    );

    CREATE TABLE [#TempTaxValues]
    (
        [BetslipId] [VARCHAR](50) NOT NULL,
        [DateTimeStamp] [DATETIME] NULL,
        [Number] [INT] NULL,
        [Stake] [DECIMAL](38, 4) NULL,
        [WinningStake] [DECIMAL](38, 4) NULL,
        [Payout] [DECIMAL](38, 4) NULL,
        [GGR] [DECIMAL](38, 4) NULL,
        [Tax Amount] [NUMERIC](38, 6) NULL,
        [VAT] [NUMERIC](38, 6) NULL,
        [GameType] [VARCHAR](12) NOT NULL,
        [Date] [DATETIME] NULL,
        [ResultingStatus] [VARCHAR](50) NULL
    );



    SET @VATPerc = CASE
                       WHEN @StartDate &lt; ''01-Apr-2018'' THEN
                           14.00
                       ELSE
                           15.00
                   END;



    ----------------------------------------------------------
    -- #Wagers
    ----------------------------------------------------------
    INSERT INTO [#Wagers]
    (
        [BetslipId],
        [Date],
        [Amount],
        [TransactionTypeCategory],
        [SettlementStatus],
        [NoOfBets],
        [TransactionType]
    )
    SELECT [SettledWagers].[BetslipId],
           [SettledWagers].[Date],
           [SettledWagers].[Amount],
           [SettledWagers].[TransactionTypeCategory],
           [SettledWagers].[SettlementStatus],
           [SettledWagers].[NoOfBets],
           [SettledWagers].[TransactionType]
    FROM
    (
        --Current Betslip, Current Tlog
        SELECT [a].[BetslipId],
               ISNULL([a].[InitialSettlementDateTime], [a].[SettlementDateTime]) AS [Date],
               [a].[WageredAmount] AS [Amount],
               ''Settled Wagers'' AS [TransactionTypeCategory],
               [e].[Description] AS [SettlementStatus],
               [a].[NoOfBets],
               [tt].[Description] AS [TransactionType]
        FROM [VuvuzelaZA].[dbo].[Betslip] AS [a] (NOLOCK)
            JOIN [VuvuzelaZA].[dbo].[TransactionLog] AS [tl] (NOLOCK)
                ON [tl].[InternalReferenceId] = [a].[BetslipId]
                   AND [tl].[TransactionTypeId] IN ( ''00000000-0000-0000-DA7A-000000600001'',
                                                     ''00000000-0000-0000-DA7A-000000600002''
                                                   )
            JOIN [VuvuzelaZA].[dbo].[TransactionType] AS [tt]
                ON [tt].[TransactionTypeId] = [tl].[TransactionTypeId]
            JOIN [VuvuzelaZA].[dbo].[SettlementStatus] AS [e] (NOLOCK)
                ON [a].[SettlementStatus] = [e].[SettlementStatusId]
                   AND [e].[Entity] = ''Betslip''
                   AND [e].[SettlementStatusId] IN ( 3, 5, 9 )
        WHERE [a].[IsDeleted] = 0
              AND ISNULL(a.[InitialSettlementDateTime], a.[SettlementDateTime])
              BETWEEN @StartDate AND @EndDate
     
    ) AS [SettledWagers];


    ----------------------------------------------------------
    -- PayoutWagers
    ----------------------------------------------------------
	SELECT @LastArcEntry = MAX([tl].[EditedDateTime])
    FROM [VuvuzelaZA].[Archive].[TransactionLog] AS [tl] (NOLOCK);

	INSERT INTO [#PayoutWagers]
    (
        [BetslipId],
        [Date],
        [Amount],
        [TransactionType],
        [TransactionTypeCategory]
    )

   SELECT IIF(TransactionType LIKE ''Live%'' OR TransactionType LIKE ''Casino%'',
               ''00000000-0000-0000-0000-''
               + RIGHT(''000000000000''
                       + CONVERT(
                                    VARCHAR,
                                    ROW_NUMBER() OVER (PARTITION BY d.InternalReferenceId ORDER BY d.InternalReferenceId)
                                ), 12),
               InternalReferenceId) AS BetslipId,
           [d].[Date],
           [d].[Amount],
           [d].[Transactiontype],
           [d].[TransactiontypeCategory]
    FROM
    (
        SELECT [a].[InternalReferenceId],
               CASE
                   WHEN [a].[TransactionTypeId] IN ( ''00000000-0000-0000-DA7A-000000600032'',
                                                     ''00000000-0000-0000-DA7A-000000600033'',
                                                     ''00000000-0000-0000-DA7A-000000600047'',
                                                     ''00000000-0000-0000-DA7A-000000600049'',
                                                     ''00000000-0000-0000-DA7A-000000600079''
                                                   ) THEN
                       DATEADD(hh, -2, [a].[EditedDateTime])
                   ELSE
                       [a].[EditedDateTime] --&gt; All Other data GMT+2 colossus date shift
               END AS [Date],
               [a].[Amount] AS [Amount],
               [b].[Description] AS [Transactiontype],
               [c].[Description] AS [TransactiontypeCategory]
        FROM [VuvuzelaZA].[dbo].[TransactionLog] AS [a] (NOLOCK)
            INNER JOIN [VuvuzelaZA].[dbo].[TransactionType] AS [b] (NOLOCK)
                ON [a].[TransactionTypeId] = [b].[TransactionTypeId]
            INNER JOIN [VuvuzelaZA].[dbo].[TransactionTypeCategory] AS [c] (NOLOCK)
                ON [b].[TransactionTypeCategoryId] = [c].[TransactionTypeCategoryId]
        WHERE [a].[EditedDateTime]
              BETWEEN @StartDate AND @EndDate
              AND [a].[IsDeleted] = 0
              AND b.TransactionTypeCategoryId IN ( ''00000000-0000-0000-DA7A-000000500001'',
                                                   ''00000000-0000-0000-DA7A-000000500002''
                                                 )
              AND b.BalanceIndexAffected = 0
			  		and NOT EXISTS (SELECT TOP 1 1 FROM VuvuzelaZA.synapse.betslip	  SB WHERE 	a.InternalReferenceId = Sb.InternalReference) --Remove horse racing
        --Winboost
        UNION ALL
        SELECT [a].[InternalReferenceId],
               CASE
                   WHEN [a].[TransactionTypeId] IN ( ''00000000-0000-0000-DA7A-000000600032'',
                                                     ''00000000-0000-0000-DA7A-000000600033'',
                                                     ''00000000-0000-0000-DA7A-000000600047'',
                                                     ''00000000-0000-0000-DA7A-000000600049'',
                                                     ''00000000-0000-0000-DA7A-000000600079''
                                                   ) THEN
                       DATEADD(hh, -2, [a].[EditedDateTime])
                   ELSE
                       [a].[EditedDateTime] --&gt; All Other data GMT+2
               END AS [Date],
               [a].[Amount] AS [Amount],
               [b].[Description] AS [Transactiontype],
               [c].[Description] AS [TransactiontypeCategory]
        FROM [VuvuzelaZA].[dbo].[TransactionLog] AS [a] (NOLOCK)
            INNER JOIN [VuvuzelaZA].[dbo].[TransactionType] AS [b] (NOLOCK)
                ON [a].[TransactionTypeId] = [b].[TransactionTypeId]
            INNER JOIN [VuvuzelaZA].[dbo].[TransactionTypeCategory] AS [c] (NOLOCK)
                ON [b].[TransactionTypeCategoryId] = [c].[TransactionTypeCategoryId]
        WHERE [a].[EditedDateTime]
              BETWEEN @StartDate AND @EndDate
              AND [a].[IsDeleted] = 0
              AND [a].TransactionTypeId = ''00000000-0000-0000-DA7A-000000600077''
              AND [a].[EditedDateTime] &gt;= ''2020-10-26''
              AND b.BalanceIndexAffected = 0
