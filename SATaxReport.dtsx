                       );


    INSERT INTO #TempTaxValues
    (
        [BetslipId],
        [DateTimeStamp],
        [Number],
        [Stake],
        [WinningStake],
        [Payout],
        [GGR],
        [Tax Amount],
        [VAT],
        [GameType],
        [Date],
        [ResultingStatus]
    )
    SELECT [sq].[BetslipId],
           [sq].[DateTimeStamp] AS [DateTimeStamp],
           sq.Number,
           [sq].[Stake],
           [sq].[WinningStake],
           [sq].[Payout],
           [sq].[GGR],
           [sq].[Tax Amount],
           [sq].[VAT],
           [sq].[TransactionTypeCategory] AS [GameType],
           [sq].[Date],
           ISNULL([rs].[Description], sq.[Status]) AS [ResultingStatus]
    FROM
    (
        SELECT SUM([Final].Number) AS Number,
               SUM([Final].[Stake]) AS [Stake],
               SUM([Final].[WinningStake]) AS [WinningStake],
               SUM([Final].[Payout]) AS [Payout],
               ISNULL(SUM([Final].[Stake]) - SUM([Final].[Payout]), 0.0) AS [GGR],
               ISNULL((SUM([Final].[Stake]) - SUM([Final].[Payout])) * 0.065, 0.0) AS [Tax Amount],
               ISNULL(
                         ((ISNULL(SUM([Final].[Stake]), 0) - ISNULL(SUM([Final].[Payout]), 0))
                          - ((ISNULL(SUM([Final].[Stake]), 0) - ISNULL(SUM([Final].[Payout]), 0)) * 0.065)
                         ) * @VATPerc
                         / (100 + @VATPerc),
                         0.0
                     ) AS [VAT],
               [Final].[Date],
               CASE
                   WHEN [Final].[TransactionTypeCategory] LIKE ''%BetGame%'' THEN
                       ''Betgames''
                   WHEN [Final].[TransactionTypeCategory] LIKE ''%Lucky%'' THEN
                       ''LuckyNumbers''
                   WHEN [Final].[TransactionTypeCategory] LIKE ''%Live%'' or [Final].[TransactionTypeCategory] LIKE ''%Casino%'' THEN
                       ''LiveGames''
                   WHEN [Final].[TransactionTypeCategory] LIKE ''%JikaSport%'' or [Final].[TransactionTypeCategory] LIKE ''%Virt%'' THEN
                       ''Jika''
                   WHEN [Final].[TransactionTypeCategory] LIKE ''%Win Boost Cash Bonus%'' THEN
                       ''WinBoost''
                   ELSE
                       ''Sports''
               END AS [TransactionTypeCategory],
               [Final].[BetslipId],
               [Final].[DateTimeStamp],
               Final.[Status]
        FROM
        (
            SELECT 0 AS Number,
                   0 AS [Stake],
                   ISNULL([W].[WinningStake], 0.0) AS [WinningStake],
                   0 AS [Payout],
                   0 AS [GGR],
                   0 AS [Tax Amount],
                   0 AS [VAT],
                   [W].[Date] AS [Date],
                   [W].[TransactionTypeCategory] AS [TransactionTypeCategory],
                   [W].[BetslipId],
                   [Date] AS [DateTimeStamp],
                   NULL AS [Status]
            FROM [#WinningStake] AS [W]
            UNION ALL
            SELECT SUM([NoOfBets]) AS Number,
                   SUM(ISNULL([Stake].[Amount], 0.0)) AS [Stake],
                   0 AS [WinningStake],
                   0 AS [Payout],
                   0 AS [GGR],
                   0 AS [Tax Amount],
                   0 AS [VAT],
                   CAST([Date] AS DATE) AS [Date],
                   [Stake].[TransactionType] AS [TransactionTypeCategory],
                   [Stake].[BetslipId],
                   [Date] AS [DateTimeStamp],
                   [ResultingStatus] AS [Status]
            FROM [#ALLWagers] AS [Stake]
            GROUP BY CAST([Date] AS DATE),
                     [Stake].[TransactionType],
                     [Stake].[BetslipId],
                     [Date],
                     [ResultingStatus]
            UNION ALL
            SELECT 0 AS Number,
                   0 AS [Stake],
                   0 AS [WinningStake],
                   SUM(ISNULL([Payout].[Amount], 0.0)) AS [Payout],
                   0 AS [GGR],
                   0 AS [Tax Amount],
                   0 AS [VAT],
                   CAST([Date] AS DATE) AS [Date],
                   [Payout].[TransactionType] AS [TransactionTypeCategory],
                   [Payout].[BetslipId],
                   [Date] AS [DateTimeStamp],
                   NULL AS [Status]
            FROM [#PayoutWagers] AS [Payout]
            WHERE [TransactionTypeCategory] = ''Payout''
            GROUP BY CAST([Date] AS DATE),
                     [Payout].[TransactionType],
                     [Payout].[BetslipId],
                     [Payout].[Date]
            UNION ALL
            SELECT 0 AS Number,
                   0 AS [Stake],
                   0 AS [WinningStake],
                   SUM(ISNULL([Payout].[Amount], 0.0)) AS [Payout],
                   0 AS [GGR],
                   0 AS [Tax Amount],
                   0 AS [VAT],
                   CAST([Date] AS DATE) AS [Date],
                   [Payout].[TransactionType] AS [TransactionTypeCategory],
                   [Payout].[BetslipId],
                   [Date] AS [DateTimeStamp],
                   NULL AS [Status]
            FROM [#PayoutWagers] AS [Payout]
            WHERE [TransactionType] = ''Win Boost Cash Bonus''
            GROUP BY CAST([Date] AS DATE),
                     [Payout].[TransactionType],
                     [Payout].[BetslipId],
                     [Payout].[Date]
        ) AS [Final]
        GROUP BY [Final].[Date],
                 [Final].[TransactionTypeCategory],
                 [Final].[BetslipId],
                 [Final].[DateTimeStamp],
                 Final.[Status]
    ) AS [sq]
        LEFT JOIN [VuvuzelaZA].[dbo].[Betslip] AS [bs]
            ON [bs].[BetslipId] = [sq].[BetslipId]
               AND ISNULL([bs].[InitialSettlementDateTime], [bs].[SettlementDateTime])
               BETWEEN @StartDate AND @EndDate
        LEFT JOIN [VuvuzelaZA].[dbo].[ResultingStatus] AS [rs]
            ON [rs].[ResultingStatusId] = [bs].[ResultingStatus]
               AND [rs].[Entity] = ''Betslip'';




   INSERT INTO #Merge
    (
